
public class DNAMaxNucleotide {
	int numberNuc;
	int maxNuc;
	String result;
    public String max(String[] strands, String nuc) {
            for (int i = 0; i < strands.length; i++)
            {
            		for(int j = 0; j < strands[i].length(); j++)
            		{
            			String[] letters = new String[(strands[i].length())];
            			letters[j] = strands[i].substring(j, j+1);
            			if (letters[j].equals(nuc))
            			{
            				numberNuc++;
            			}
            		}
            		if (numberNuc > maxNuc)
            		{
            			maxNuc = numberNuc;
            			result = strands[i];
            		}
            		if (numberNuc == maxNuc)
            		{
            			if(strands[i].length() > result.length())
            			{
            				result = strands[i];
            			}
            		}
            }
            return result;
      }
   }