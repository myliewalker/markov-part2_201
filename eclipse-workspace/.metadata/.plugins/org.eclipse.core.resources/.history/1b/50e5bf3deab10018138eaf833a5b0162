import java.util.ArrayList;

public class Common {

	public int count (String a, String b) {
		ArrayList<String> common = new ArrayList();
		int count = 0;
        for (int i = 0; i < a.length(); i++)
        {
        		for (int j = 0; j < b.length(); j++)
        		{
        			if (a.substring(i, i+1).equals(b.substring(j, j+1)))
        			{
        				if (repeat(i, a) == true)
        				{
        					if (repeat(b) == true)
        					{
        						count++;
        					}
        				}
        				else
        				{
        					count++;
        				}
//        				count++;
//        				common.add(a.substring(i, i+1));
        				break;
        			}
        		}
        }
//        for (int k = 0; k < common.size(); k++)
//        {
//        		for (int m = 0; m < common.size(); m++)
//        		{
//        			if(common.get(k).equals(common.get(m)));
//        			{
//        				common.remove(k);
//        			}
//        		}
//        }
        return count;
     }
	
	public boolean repeat (int pos, String c)
	{
		for (int i = 0; i < c.length(); i++)
		{
			for (int j = i+1; j < c.length(); j++)
			{
				if (c.substring(i, i+1).equals(c.substring(j, j+1)) && i != pos)
				{
					return true;
				}
			}
		}
		return false;
	}
	
	public boolean repeat (String c)
	{
		for (int i = 0; i < c.length(); i++)
		{
			for (int j = i+1; j < c.length(); j++)
			{
				if (c.substring(i, i+1).equals(c.substring(j, j+1)))
				{
					return true;
				}
			}
		}
		return false;
	}
}
